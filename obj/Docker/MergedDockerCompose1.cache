services:
  blog.database:
    container_name: blog.databse
    environment:
      POSTGRES_DB: blog
      POSTGRES_PASSWORD: password
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\BABATUNDE\source\repos\BlogApi\.containers\blog-db
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
